                            'icon-ignore-placement': true,
                            'icon-padding': 0,
                        }
                    });
                });

                mapbox_map.loadImage('/images/sykes/markers/map-cycling-route-icon.png', function (error, image) {
                    if (error) throw error;
                    mapbox_map.addImage('custom-marker-route-cycling', image);
                    mapbox_map.addLayer({
                        id: 'Cycling_Points',
