	<script type="text/javascript">
		$("a[href='#map_wrp']").on('click', function(event) {
			eventTracker.fireEvent('Property Page', 'Scroll To Map', 'kep-5979');
			if (this.hash !== "") {
				event.preventDefault();
				var hash = this.hash;
				$('html, body').animate({
					scrollTop: $(hash).offset().top
				}, 800, function() {
					window.location.hash = hash;
				});
			}
		});
	</script>


