        document.addEventListener("DOMContentLoaded", () => {
            const postHogElements = document.querySelectorAll('[data-posthog-event]');
            const postHogPageloadElements = document.querySelectorAll('[data-posthog-pageload-event]');

            postHogElements.forEach(element => {
                element.addEventListener('click', (event) => {
                    const target = event.target instanceof Element
                        ? event.target.closest('[data-posthog-event]')
                        : null;
                    if (target) {
                        try {
                            let eventContents = target.getAttribute('data-posthog-event');
                            if (eventContents && eventContents.trim().charAt(0) === "'") {
                                eventContents = eventContents.replace(/'/g, '"');
                            }
                            eventContents = eventContents.trim();
                            const eventObj = JSON.parse(eventContents);

                            if (target.getAttribute('data-posthog-include-form-values')) {
                                const includeValues = JSON.parse(target.getAttribute('data-posthog-include-form-values'));
                                eventObj.data.formValues = includeValues.reduce((acc, fieldName) => {
                                    const field = document.querySelector(`[name="${fieldName}"]`);
                                    if (field) {
                                        acc[fieldName] = field.value;
                                    }
                                    return acc;
                                }, {});
                            }

                            if (typeof posthog !== 'undefined' && typeof posthog.capture === 'function') {
                                posthog.capture(eventObj.event, eventObj.data);
                            }
                        } catch (error) {
                            console.error('Error capturing PostHog event:', error, target.getAttribute('data-posthog-event'));
                        }
                    }
                }, true);
            });

            postHogPageloadElements.forEach(element => {
                try {
                    let eventContents = element.getAttribute('data-posthog-pageload-event');
                    if (eventContents && eventContents.trim().charAt(0) === "'") {
                        eventContents = eventContents.replace(/'/g, '"');
                    }
                    eventContents = eventContents.trim();
                    const eventObj = JSON.parse(eventContents);
                    if (typeof posthog !== 'undefined' && typeof posthog.capture === 'function') {
                        posthog.capture(eventObj.event, eventObj.data);
                    }
                } catch (error) {
                    console.error('Error capturing PostHog pageload event:', error, element.getAttribute('data-posthog-pageload-event'));
                }
            });
        });
    </script>
